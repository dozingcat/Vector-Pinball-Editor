    "name": "Compound Interest",
    "numballs": 3,
    "script": "// Initialized in gameStarted.\u000adef launchBarrier = null;\u000adef topBlocker = null;\u000adef rightKicker = null;\u000adef multiballKickers = null;\u000adef lockedBallRollovers = null;\u000adef lockedBallKickers = null;\u000adef inMultiball = false;\u000adef isMultiballStarting = false;\u000adef bumperMultiplierIncrease = 1;\u000a\u000adef colorSuffixes = [\"Blue\", \"Cyan\", \"Green\", \"Yellow\", \"Red\", \"Magenta\"];\u000adef multiballStatusRollovers = [:]\u000adef ballsLocked = 0;\u000a// Lighting all rollovers in multiball scores jackpot*1000 points\u000a// and increases multiplier by (jackpot) percent.\u000adef jackpot = 0;\u000adef baseJackpot = 25;\u000adef jackpotIncrease = 5;\u000a\u000adef allStatusRolloversActiveForIndex = {index ->\u000a  multiballStatusRollovers.every {key,rollovers -> rollovers.isRolloverActiveAtIndex(index)}\u000a};\u000a\u000adef setAllMultiballStatusRolloversActive = {active ->\u000a  multiballStatusRollovers.each {suffix, rollover ->\u000a    (0..2).forEach {index ->\u000a      rollover.setRolloverActiveAtIndex(index, active);\u000a    };\u000a  }\u000a};\u000a\u000adef setIgnoreBallForMultiballStatusRollovers = {ignoreBall ->\u000a  multiballStatusRollovers.each {suffix, rollover ->\u000a    rollover.setIgnoreBall(ignoreBall);\u000a  };\u000a};\u000a\u000adef clearMultiballStatus = {\u000a  setAllMultiballStatusRolloversActive(false);\u000a  setIgnoreBallForMultiballStatusRollovers(true);\u000a  ballsLocked = 0;\u000a  inMultiball = false;\u000a  isMultiballStarting = false;\u000a};\u000a\u000adef increaseExtraBumperMultiplier = {field, percent ->\u000a  bumperMultiplierIncrease += percent;\u000a  field.showGameMessage(\"Bumper Multiplier \" + bumperMultiplierIncrease + \"%\", 1500);\u000a};\u000a\u000adef startMultiball = {field ->\u000a  field.showGameMessage(\"Multiball!\", 3000);\u000a  lockedBallRollovers[2].setIgnoreBall(true);\u000a  lockedBallRollovers[2].setVisible(false);\u000a  ballsLocked = 3;\u000a  inMultiball = true;\u000a  isMultiballStarting = true;\u000a  setAllMultiballStatusRolloversActive(false);\u000a  setIgnoreBallForMultiballStatusRollovers(false);\u000a  jackpot = baseJackpot;\u000a\u000a  field.scheduleAction(1000, {\u000a    lockedBallKickers[1].setRetracted(true);\u000a    lockedBallKickers[2].setRetracted(true);\u000a    lockedBallRollovers[1].setIgnoreBall(true);\u000a    lockedBallRollovers[1].setVisible(false);\u000a    if (field.getBalls().size() < 3) field.launchBall();\u000a  });\u000a  field.scheduleAction(3500, {\u000a    isMultiballStarting = false;\u000a    lockedBallRollovers[0].setIgnoreBall(true);\u000a    lockedBallRollovers[0].setVisible(false);\u000a    if (field.getBalls().size() < 3) field.launchBall();\u000a  });\u000a};\u000a\u000adef doJackpot = {field ->\u000a  field.addScore(jackpot * 1000);\u000a  field.showGameMessage(\"Jackpot!\", 3000);\u000a  // Increase multiplier by jackpot percentage. This can actually\u000a  // cause an overflow if the player scores a whole lot of jackpots.\u000a  def multiplier = field.getScoreMultiplier();\u000a  def newMultiplierPercent = (multiplier * (100+jackpot)).longValue();\u000a  field.setScoreMultiplier((newMultiplierPercent/100.0).doubleValue());\u000a  jackpot += jackpotIncrease;\u000a  setAllMultiballStatusRolloversActive(false);\u000a};\u000a\u000a// These methods are called for specific game events.\u000areturn [\u000a  gameStarted: {field ->\u000a    // Initialize common variables.\u000a    launchBarrier = field.getFieldElementById(\"LaunchBarrier\");\u000a    topBlocker = field.getFieldElementById(\"TopRampBlocker\");\u000a    rightKicker = field.getFieldElementById(\"RightRampKicker\");\u000a    multiballKickers = [\u000a      field.getFieldElementById(\"MultiballKicker1\"),\u000a      field.getFieldElementById(\"MultiballKicker2\"),\u000a      field.getFieldElementById(\"MultiballKicker3\"),\u000a    ];\u000a    // Locked ball rollovers start hidden and disabled.\u000a    lockedBallRollovers = [\u000a      field.getFieldElementById(\"LockedBallRollover1\"),\u000a      field.getFieldElementById(\"LockedBallRollover2\"),\u000a      field.getFieldElementById(\"LockedBallRollover3\"),\u000a    ];\u000a    lockedBallRollovers.each {\u000a      it.setRolloverActiveAtIndex(0, false);\u000a      it.setVisible(false);\u000a      it.setIgnoreBall(true);\u000a    };\u000a    // Kickers in the ball lock zone start disabled except the bottom one.\u000a    lockedBallKickers = [\u000a      field.getFieldElementById(\"LockedBallKicker1\"),\u000a      field.getFieldElementById(\"LockedBallKicker2\"),\u000a      field.getFieldElementById(\"LockedBallKicker3\"),\u000a    ];\u000a    lockedBallKickers[1].setRetracted(true);\u000a    lockedBallKickers[2].setRetracted(true);\u000a\u000a    // Get references to multiball status rollovers and initialize them.\u000a    colorSuffixes.each {suffix ->\u000a      multiballStatusRollovers[suffix] = field.getFieldElementById(\"Rollovers.\" + suffix);\u000a    };\u000a    clearMultiballStatus();\u000a\u000a    // Remove the launch barrier.\u000a    launchBarrier.setRetracted(true);\u000a    rightKicker.setRetracted(true);\u000a  },\u000a\u000a  ballLost: {field ->\u000a    // Restore the launch barrier.\u000a    bumperMultiplierIncrease = 1;\u000a    launchBarrier.setRetracted(false);\u000a  },\u000a\u000a  ballInSensorRange: {field, sensor, ball ->\u000a    def sensorId = sensor.getElementId();\u000a    // Check for retracting or restoring the launch barrier.\u000a    if (sensorId == 'LaunchBarrierRetract') {\u000a      launchBarrier.setRetracted(true);\u000a    }\u000a    else if (sensorId == 'TopRampSensor') {\u000a      launchBarrier.setRetracted(false);\u000a      topBlocker.setRetracted(true);\u000a      rightKicker.setRetracted(false);\u000a    }\u000a    else if (sensorId == \"RightRampSensor\") {\u000a      topBlocker.setRetracted(false);\u000a      rightKicker.setRetracted(true);\u000a    }\u000a    else if (sensorId == \"AfterRampKickerSensor\") {\u000a      topBlocker.setRetracted(false);\u000a      rightKicker.setRetracted(false);\u000a    }\u000a  },\u000a\u000a  allDropTargetsInGroupHit: {field, targetGroup ->\u000a    // Activate ball saver for left and right groups.\u000a    def id = targetGroup.getElementId();\u000a    if (id == \"DropTargetLeftSave\") {\u000a      field.getFieldElementById('BallSaver-left').setRetracted(false);\u000a      field.showGameMessage('Left Save Enabled', 1500);\u000a    }\u000a    else if (id == \"DropTargetRightSave\") {\u000a      field.getFieldElementById('BallSaver-right').setRetracted(false);\u000a      field.showGameMessage('Right Save Enabled', 1500);\u000a    }\u000a    else if (id == \"DropTargetTop\") {\u000a      increaseExtraBumperMultiplier(field, 1);\u000a    }\u000a  },\u000a\u000a  processCollision: {field, element, body, ball ->\u000a    def id = element.getElementId();\u000a    if (id!=null && id.startsWith(\"Bumper.\")) {\u000a      def suffix = id.substring(7);\u000a      // Increment multiplier.\u000a      def multiplier = Math.round(field.getScoreMultiplier()*100).longValue();\u000a      multiplier += bumperMultiplierIncrease;\u000a      field.setScoreMultiplier((multiplier/100).doubleValue());\u000a      // Unlock next multiball target if all bumpers hit.\u000a      if (ballsLocked < 3) {\u000a        def rollovers = multiballStatusRollovers[suffix];\u000a        rollovers.setRolloverActiveAtIndex(ballsLocked, true);\u000a        if (allStatusRolloversActiveForIndex(ballsLocked)) {\u000a          // If rollover for ball lock isn't already active, activate and show message.\u000a          if (lockedBallRollovers[ballsLocked].getIgnoreBall()) {\u000a            lockedBallRollovers[ballsLocked].setIgnoreBall(false);\u000a            lockedBallRollovers[ballsLocked].setVisible(true);\u000a            lockedBallRollovers[ballsLocked].setRolloverActiveAtIndex(0, false);\u000a            def msg = (ballsLocked == 2) ? \"Multiball Ready\" : \"Ball Lock Ready\";\u000a            field.showGameMessage(msg, 3000);\u000a          }\u000a        }\u000a      }\u000a    }\u000a  },\u000a\u000a  allRolloversInGroupActivated: {field, rollovers ->\u000a    if (rollovers == lockedBallRollovers[0]) {\u000a      field.removeBallWithoutBallLoss(field.getBalls()[0]);\u000a      lockedBallKickers[1].setRetracted(false);\u000a      field.showGameMessage(\"Ball 1 Locked\", 3000);\u000a      ballsLocked = 1;\u000a    }\u000a    else if (rollovers == lockedBallRollovers[1]) {\u000a      field.removeBallWithoutBallLoss(field.getBalls()[0]);\u000a      lockedBallKickers[2].setRetracted(false);\u000a      field.showGameMessage(\"Ball 2 Locked\", 3000);\u000a      ballsLocked = 2;\u000a    }\u000a    else if (rollovers == lockedBallRollovers[2]) {\u000a      startMultiball(field);\u000a    }\u000a    else if (inMultiball && multiballStatusRollovers.containsValue(rollovers)) {\u000a      // Score jackpot if all rollovers have been activated.\u000a      def isJackpot = multiballStatusRollovers.every {suffix, mr ->\u000a        mr.allRolloversActive();\u000a      };\u000a      if (isJackpot) {\u000a        doJackpot(field);\u000a      }\u000a    }\u000a    else if (rollovers.getElementId() == \"FlipperRollovers\") {\u000a      increaseExtraBumperMultiplier(field, 2);\u000a      rollovers.setAllRolloversActivated(false);\u000a    }\u000a  },\u000a\u000a  tick: {field, nanos ->\u000a    if (inMultiball && !isMultiballStarting && field.getBalls().size()<=1) {\u000a      clearMultiballStatus();\u000a    }\u000a  },\u000a]",
    "targetTimeRatio": "2.3",
    "version": 1,
    "width": "20"
}